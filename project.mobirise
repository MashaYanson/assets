lbar=\"-mbrBtnMove\"><a class=\"btn btn-secondary-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Введите Текст\">\n                            <span class=\"mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn\"></span>\n                            metaphora@email.com\n                        </a></div>\n                    <div class=\"text-wrapper\">\n                        <p class=\"mbr-phone mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showPhone\" data-app-selector=\".mbr-phone, .text-wrapper\">\n                            <br>+7 (999) 988-98-88</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
